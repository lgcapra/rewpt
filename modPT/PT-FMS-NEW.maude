 *** eq m1' = (2 * (sd(M,1))) . p(1,"") + 1 . p(4,"") + 1 . p(3,"") .  *** M > 1
 *** eq d1 = M . p(3,"") + 1 . p(4,"") + 1 . p(8,"") .
 *** eq d2 = M . p(2,"") + 1 . p(5,"") + 1 . p(7,"") .


 *** eq net' = t(0,"") |-> [2 . p(1,""), 1 . p(2,"") + 1 . p(3,""), nilP] ; t(1,"") |-> [1 . p(2,""), 1 . p(4,""), 1 . p(7,"") ] ; t(2,"") |-> [1 . p(3,""), 1 . p(5,""),
