 ops sys0  PLsys 2PLsys : -> System .
 ops slinef 2sfaultylines : -> SimpleSys .
 ops m0 m0' m1 m1' d1 d2 sym0 : -> Pbag .
 op M : -> NzNat . *** MS model's parameter
 eq M = 4 . 
 ops sys0 sys sys2 sys2' sys2'' sys2new sys2m1 sys2m2 sys4 : -> System .

 eq sline =  [1 . p(< "working" ; 0 >) , 1 . p(< "ass" ; 0 >), 1 . p(< "faulty" ; 0 >) ] .
 eq line = t(< "line" ; 0 >) |-> sline .
 eq sfault =  [1 . p(< "focc" ; 0 >) , 1 . p(< "faulty" ; 0 >), nilP ] .
 eq fault = t(< "fault" ; 0 >) |-> sfault .
 eq sload = [1 . p(< "start" ; 0 >) , 1 . p(< "working" ; 0 >) , nilP ] .
 eq load = t(< "load" ; 0 >) |-> sload .
 eq sass =  [1 . p(< "ass" ; 0 >) , 1 . p(< "start" ; 0 >)  , nilP ] .
 eq ass  = t(< "ass" ; 0 >) |-> sass .
 eq plcycle = load ; line ; ass .
 eq plcyclef = plcycle ; fault .
 eq PLf = replica@share(plcyclef, 2, "PL", "focc" U "start", "ass" U "load") .
 eq PLsys = setMark(setMark(PLf, "focc", 1), "start", 4) . 
 eq 2PLf = replica@share(PLf, 2, "sys", "start", emptyStr) .
 eq 2PLsys = setMark(setMark(2PLf, "focc" "sys", 1), "start", 4) . 
