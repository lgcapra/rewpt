 ***          t(4,"") |-> [1 . p(6,"") , 2 . p(1,""), nilP ] ; t(5,"") |-> [1 . p(0,"") + 1 . p(3,""), 1 . p(7,""), nilP ] ; t(6,"") |-> [1 . p(0,""), 1 . p(8,""), nilP].
 
 
 *** specification using node labels 
 *** versione originale (con "rest")
 eq net' = t(< "load" ; 0 >) |-> [2 . p(< "start" ; 1 >), 1 . p(< "line" ; 2 >) + 1 . p(< "line" ; 3 >), nilP ] ; t(< "line" ; 1 >) |-> [1 . p(< "line" ; 2 >) , 1 . p(< "ass" ; 4 >), 1 . p(< "faulty" ; 7 >) ] ; t(< "line" ; 2 >) |-> [1 . p(< "line" ; 3 >) , 1 . p(< "ass" ; 5 >),
          1 . p(< "faulty" ; 8 >) ] ; t(< "ass" ; 3 >) |-> [1 . p(< "ass" ; 4 >) + 1 . p(< "ass" ; 5 >) , 1 . p(< "end" ; 6 >), nilP ] ;
          t(< "rest" ; 4 >) |-> [1 . p(< "end" ; 6 >) , 2 . p(< "start" ; 1 >), nilP ] ; t(< "fault" ; 5 >) |-> [1 . p(< "focc" ; 0 >) , 1 . p(< "faulty" ; 7 >), nilP ] ; t(< "fault" ; 6 >) |-> [1 . p(< "focc" ; 0 >) , 1 . p(< "faulty" ; 8 >), nilP].
 *** versione "compatta" ("rest" assorbito da "ass")
 eq net = t(< "load" ; 0 >) |-> [2 . p(< "start" ; 1 >), 1 . p(< "line" ; 2 >) + 1 . p(< "line" ; 3 >), nilP ] ; t(< "line" ; 1 >) |-> [1 . p(< "line" ; 2 >) , 1 . p(< "ass" ; 4 >), 1 . p(< "faulty" ; 7 >) ] ; t(< "line" ; 2 >) |-> [1 . p(< "line" ; 3 >) , 1 . p(< "ass" ; 5 >),
          1 . p(< "faulty" ; 8 >) ] ; t(< "ass" ; 3 >) |-> [1 . p(< "ass" ; 4 >) + 1 . p(< "ass" ; 5 >) , 2 . p(< "start" ; 1 >), nilP ] ;
          t(< "fault" ; 5 >) |-> [1 . p(< "focc" ; 0 >) , 1 . p(< "faulty" ; 7 >), nilP ] ; t(< "fault" ; 6 >) |-> [1 . p(< "focc" ; 0 >) , 1 . p(< "faulty" ; 8 >), nilP].
 eq m0  = (2 * M) . p(< "start" ; 1 >) +  1 . p(< "focc" ; 0 >) .

 eq sys0 = net m0 .

 eq snet = toSimpleNet(net) .

