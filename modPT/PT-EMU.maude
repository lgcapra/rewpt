in PT-SYS

mod PT-EMU is
 pr PT-SYS .
 var  T : Tran .
 vars I O H M : Pbag .
 vars N N' : Net .
 *** firing rule (without canonization)
 crl [firing] : N M => N M + O - I if  T |-> [I,O,H] ; N' := N /\ I  =/= O /\ I <= M /\ H >' M .
 *** including canonization
 *** crl [canfire] : N M => canonize(N M + O - I) if  T |-> [I,O,H] ; N' := N /\ I  =/= O /\ I <= M /\ H >' M .
endm
