fmod LAB-EDGE{X :: TRIV, L :: TRIV, Y :: TRIV} is 
 sort Edge{X, L, Y} . 
 op ((_,_,_)) : X$Elt L$Elt Y$Elt -> Edge{X, L, Y} . 
 op s_ : Edge{X, L, Y} -> X$Elt . 
 op t_ : Edge{X, L, Y} -> Y$Elt . 
 op l_ : Edge{X, L, Y} -> L$Elt .

 var A : X$Elt . 
 var B : L$Elt .
 var C : Y$Elt . 
 eq s (A, B, C) = A .
 eq t (A, B, C) = C . 
 eq l (A, B, C) = B . 
endfm